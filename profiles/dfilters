"Bad TCP - only" tcp.analysis.flags && !tcp.analysis.window_update && !(tcp.analysis.keep_alive or tcp.analysis.keep_alive_ack)
"Eth broadcast" eth.addr == ff:ff:ff:ff:ff:ff
"Ethernet broadcast" eth.addr == ff:ff:ff:ff:ff:ff
"Ethernet type 0x0806 (ARP)" eth.type == 0x0806
"Flags Any Data in TCP Urgent Field?" tcp.urgent_pointer>0
"Flags FIN"  tcp.flags & 1
"Flags Initial SYN" tcp.flags.syn == 1 && tcp.flags.ack==0
"Flags Push Bit is set" tcp.flags.push == 1
"Flags RST"  tcp.flags & 4
"Flags SFR" tcp.flags & 7
"Flags SYN packets" (tcp.flags.syn == 1) && (tcp.flags.ack ==0)
"Flags SYN plus ACK (Bitwise)" tcp.flags & 0x12
"Flags SYN" tcp.flags & 2
"IPv6 only" ipv6
"Low TCP Win" tcp.window_size =< 1460
"Packets with Comments" frame.comment
"QUIC Connection Number" quic.connection.number == 1
"QUIC only" quic
"Retrans" tcp.analysis.retransmission || tcp.analysis.fast_retransmission
"Slow HTTP" frame.time_delta_displayed > 1 and (http.request.method == "GET" || http.response.code == 200)
"Syntax Correct syntax for not IP addr" !(ip.addr == 192.168.0.1)
"Syntax Port 443 or 4430 thru 4434" tcp.port in {443 4430..4434}
"Syntax specify different ports in { }"  not tcp.port in {80 25}
"TCP Delta Times greater than 1 second" tcp.time_delta > 1
"TCP Length >0" tcp.len > 0
"TCP Retransmissions" tcp.analysis.retransmission or tcp.analysis.out_of_order
"TLS Certificate Packets" tls.handshake.type == 11
"TLS Client Hello Packets" tls.handshake.type == 1
"TLS Server Hello Packets" tls.handshake.type == 2
"TLS Server Key Exchange Packets" tls.handshake.type == 12
